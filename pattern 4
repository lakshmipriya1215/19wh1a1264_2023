#include <bits/stdc++.h>
using namespace std;

int main() {
	int n,i,j;
	cin>>n;
	for(i = n ; i > 1 ; i--)
	{
		for(j = 1 ; j <= i ; j++)
		{
			cout<<j;
		}
		cout<<"\n";
	}
	cout<<n<<"\n";
	for(i = 2 ; i <= n ; i++)
	{
		for(j = 1 ; j <= i ; j++)
		{
			cout<<j;
		}
		cout<<"\n";
	}
	
	// your code goes here
	return 0;
}
